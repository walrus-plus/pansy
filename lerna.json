{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "packages": ["packages/*", "examples/*"],
  "changelog": {
    "repo": "walrus-plus/pansy",
    "labels": {
      "PR: New Feature": "feat: New Features",
      "PR: Breaking Change": "boom: Breaking Changes",
      "PR: Bug Fix": "bug: Bug Fix",
      "PR: Documentation": "docs: Documentation",
      "PR: Underlying Tools": "chore: Underlying Tools"
    },
    "cacheDir": ".changelog"
  }
}
