    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": false,
    "newLine": "LF",
    "skipLibCheck": true,
    "sourceMap": true,
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true
  }
}
