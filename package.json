{
  "name": "@pansy/cli",
  "version": "0.1.0",
  "description": "A zero configuration library bundler.",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "babel.js"
  ],
  "scripts": {
    "bootstrap": "yarn && npm install",
    "types": "tsc --build tsconfig.types.json",
    "build": "rm -rf dist && node lib/cli",
    "test": "walrus test",
    "lint": "walrus lint",
    "lint:commit": "walrus commitlint --env HUSKY_GIT_PARAMS",
    "prettier": "walrus prettier",
    "prepublishOnly": "yarn types && yarn build"
  },
  "repository": "git@github.com:walrus-plus/pansy.git",
  "author": "wangxingkang <wang_xingkang@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@walrus/shared-utils": "^0.2.2",
    "babel-plugin-alter-object-assign": "^1.0.2",
    "babel-plugin-transform-async-to-promises": "^0.8.4",
    "ora": "^4.0.2",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-hashbang": "^2.2.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-typescript2": "^0.19.2"
  },
  "devDependencies": {
    "@types/boxen": "^3.0.1",
    "@types/node": "^12.12.3",
    "@types/ora": "^3.0.0",
    "@types/pify": "^3.0.2",
    "@types/resolve": "^0.0.8",
    "@walrus/cli": "^0.2.2",
    "boxen": "^4.1.0",
    "builtin-modules": "^3.1.0",
    "cac": "^6.5.3",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "gzip-size": "^5.1.1",
    "husky": "^2.5.0",
    "p-waterfall": "^2.1.0",
    "pify": "^4.0.1",
    "pretty-bytes": "^5.3.0",
    "pretty-ms": "^5.0.0",
    "require-so-slow": "^1.2.0",
    "resolve": "^1.12.0",
    "resolve-from": "^5.0.0",
    "semantic-release": "^15.13.28",
    "slash": "^3.0.0",
    "string-width": "^4.1.0",
    "stringify-author": "^0.1.3",
    "text-table": "^0.2.0",
    "typescript": "^3.6.4",
    "v8-compile-cache": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "yarn lint:commit"
    }
  },
  "browserslist": [
    "node 8"
  ],
  "engines": {
    "node": ">=8"
  },
  "watch": {
    "./src/**/*.ts": [
      "docs"
    ]
  }
}
